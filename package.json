{
  "name": "bottok",
  "version": "1.0.0",
  "description": "Bot Tok is a TikTok Up Bot (likes, shares, favorites and views) based in Node.js, Puppeteer, and third-party providers (zefoy.com & freer.es)",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo 'Build completed - Node.js application ready'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "setup": "node scripts/setup.js",
    "cli": "node scripts/cli.js"
  },
  "keywords": [
    "tiktok",
    "bot",
    "automation",
    "puppeteer",
    "views",
    "likes",
    "shares",
    "favorites"
