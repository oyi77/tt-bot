# Application Settings
NODE_ENV=development
PORT=3000
DEBUG=false

# Puppeteer Settings
HEADLESS=true
SLOW_MO=100
TIMEOUT=30000

# Zefoy Provider Settings
ZEFOY_DELAY=30000
ZEFOY_SUBMIT_DELAY=5000

# Freer Provider Settings
FREER_DELAY=25000
FREER_SUBMIT_DELAY=4000

# TikTok Settings
MAX_RETRIES=3
RETRY_DELAY=5000
MAX_CONCURRENT_TASKS=1
TASK_TIMEOUT=300000

# Session Management
COOKIE_PATH=./data/cookies.json
USER_AGENT_PATH=./data/user-agents.json
PROXY_PATH=./data/proxies.json
SESSION_TIMEOUT=3600000

# Logging
LOG_LEVEL=info
LOG_FILE=./data/logs/app.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=10
MAX_REQUESTS_PER_HOUR=100
COOLDOWN_PERIOD=3600000
